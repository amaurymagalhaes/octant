{
  "name": "wildland-governance-game",
  "author": "Wildland Team",
  "homepage": ".",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "cd .. && husky install client/.husky",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "eslint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "yarn prettier && yarn eslint --fix",
    "prettier": "yarn prettier:js --loglevel silent && yarn prettier:ts --loglevel silent",
    "prettier:js": "yarn prettier:js:pure './**/*.{js,jsx}'",
    "prettier:js:pure": "prettier --config .prettierrc.yml --write",
    "prettier:ts": "yarn prettier:ts:pure './**/*.{ts,tsx}'",
    "prettier:ts:pure": "prettier --config .prettierrc.yml --parser typescript --write",
    "type-check": "tsc --noEmit true",
    "test": "jest",
    "preinstall": "yarn remove-typechain && yarn copy-typechain",
    "remove-typechain": "rm -rf ./src/typechain-types",
    "copy-typechain": "cp -r ../typechain-types ./src/typechain-types"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "ethers": "^5.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "regenerator-runtime": "^0.13.9",
    "use-metamask": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^4.31.1",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "ts-jest": "^29.0.2",
    "typescript": "^4.8.3",
    "vite": "^3.0.0"
  },
  "engines": {
    "node": ">= 16.13.0 <17.0.0"
  },
  "lint-staged": {
    "./**/*.js": [
      "yarn prettier:js:pure",
      "eslint --fix"
    ],
    "./**/*.{ts,tsx}": [
      "yarn prettier:ts:pure",
      "eslint --fix"
    ]
  }
}
