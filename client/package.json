{
  "name": "octant-client",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/wildland/octant.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "codegen": "graphql-codegen --config codegen.ts",
    "dev": "yarn install && vite",
    "eslint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "yarn prettier && yarn eslint --fix",
    "install:rootpackages": "cd .. && yarn install && cd client",
    "predev": "yarn install:rootpackages && yarn install",
    "preinstall": "yarn install:rootpackages",
    "prettier": "yarn prettier:js --loglevel silent && yarn prettier:ts --loglevel silent",
    "prettier:js": "yarn prettier:js:pure './**/*.{js,jsx}'",
    "prettier:js:pure": "prettier --write",
    "prettier:ts": "yarn prettier:ts:pure './**/*.{ts,tsx}'",
    "prettier:ts:pure": "prettier --parser typescript --write",
    "preview": "vite preview",
    "start": "yarn build --base=/ && yarn preview --base=/ --host=0.0.0.0",
    "test": "TZ=UTC jest",
    "type-check": "tsc --noEmit true"
  },
  "dependencies": {
    "@ethersproject/constants": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@tanstack/react-query": "^4.28.0",
    "@web3modal/ethereum": "^2.2.2",
    "@web3modal/react": "^2.2.2",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "lodash": "^4.17.21",
    "minidenticons": "^2.0.2",
    "octant-typechain-types": "^0.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroller": "^1.2.6",
    "react-router-dom": "^6.7.0",
    "react-select": "^5.7.0",
    "react-toastify": "^9.1.1",
    "reselect": "^4.1.7",
    "wagmi": "^0.12.10",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@graphql-codegen/cli": "3.3.0",
    "@graphql-codegen/client-preset": "^3.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.1",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.3",
    "regenerator-runtime": "^0.13.11",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.61.0",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.0.4"
  },
  "engines": {
    "node": ">= 16.13.0 <17.0.0"
  },
  "lint-staged": {
    "./**/*.js": [
      "yarn prettier:js:pure",
      "eslint --fix"
    ],
    "./**/*.{ts,tsx}": [
      "yarn prettier:ts:pure",
      "eslint --fix"
    ]
  }
}
